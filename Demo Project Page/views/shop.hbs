<main>
    <h1 class="display-1 text-center">Shop</h1>
   


    <section class="product-grid">
  <h1>Buy Again</h1>

  {{#if products.length}}
    <div class="products">
      {{#each products}}
        {{> _product this}}
      {{/each}}
    </div>
  {{else}}
    <p>No products found.</p>
  {{/if}}
</section>
</main>
